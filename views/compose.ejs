<form id="compose" class="" action="/compose" method="post">
    <div class="fieldset">
        <label for="postTitle" class="fw-bold">Title</label>
        <input type="text" name="postTitle" autocomplete="off">
    </div>
    <div class="fieldset">
        <label for="postBody" class="fw-bold">Post</label>
