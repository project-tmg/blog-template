<%- include("partials/header"); %>

<h1>Home</h1>
<p><%= startingContent %></p>
<% posts.forEach(function(post) {%>
    <div class="posts">
        <h1><%= post.title; %></h1>
        <p class="home-post"><%= post.content.substr(0, 100) + "..."%> <a href="/posts/<%=post.title%>">Read More</a></p>
    </div>
<%});%>
<%- include("partials/footer"); %>
